        "def calculate_total_with_tax(price, tax_rate):\n",
        "    total = price + (price * tax_rate)\n",
        "    return total\n",
        "\n",
        "price = 1000\n",
        "tax_rate = 0.1\n",
        "\n",
        "total_price = calculate_total_with_tax(price, tax_rate)\n",
        "print(f\"消費税を含む合計金額: {total_price}\")"
